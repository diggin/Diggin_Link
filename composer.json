    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "psr-13"
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5"
    },
    "autoload": {
        "psr-4":{
            "Diggin\\Link\\":"src/Diggin/Link/"
        }
    },
    "autoload-dev": {
        "psr-4":{
            "DigginTest\\Link\\":"tests/DigginTest/Link/",
            "Psr\\Http\\Link\\":"tests/vendor/psr/http-link/"
        }
    }
}
